{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": ".",
    "sourceMap": false,
    "strict": true,
    "target": "es2015",
    "types": ["vite/client"]
  },
  "include": [
    "src"
  ]
}
