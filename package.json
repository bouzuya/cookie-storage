{
  "name": "cookie-storage",
  "description": "A Web Storage interface for Cookie",
  "version": "6.1.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/cookie-storage/issues"
  },
  "devDependencies": {
    "@types/node": "^10.17.14",
    "@types/power-assert": "^1.5.0",
    "@types/proxyquire": "^1.3.28",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "beater": "^9.0.0",
    "beater-helpers": "^2.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.0.5",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.0",
    "typescript": "^3.9.7",
    "watch": "^1.0.2",
    "vite": "^2.7.12"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/bouzuya/cookie-storage",
  "keywords": [
    "browser",
    "cookie",
    "storage"
  ],
  "license": "MIT",
  "main": "dist/index.umd.js",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/cookie-storage.git"
  },
  "scripts": {
    "dev":"vite",
    "build": "vite build && tsc",
    "build:format": "prettier --check '{src,test}/**/*.ts'",
    "build:lint": "eslint '{src,test}/**/*.ts'",
    "build:ts": "tsc",
    "clean": "rimraf \".tmp\" \"lib\"",
    "format": "prettier --write '{src,test}/**/*.ts'",
    "prepare": "npm-run-all -s \"clean\" \"build\"",
    "test": "npx ts-node \"./test/index.ts\"",
    "watch": "npm-run-all -p \"watch:*\"",
    "watch:test": "watch --wait 2 --verbose \"npm run test\"",
    "watch:ts": "npm run \"build:ts\" -- --watch"
  },
  "types": "./dist/src/index.d.ts"
}
